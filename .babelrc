{
    "presets": ["@babel/preset-env", "@babel/preset-react"], //next/babel next内置的模块，脚手架自带无需安装
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        ["@babel/plugin-transform-runtime"]
    ]
}
